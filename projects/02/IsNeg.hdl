 * IsNeg gate:
 * out = 1 if (in[15] == 1 )
 *       0 otherwise
 */

CHIP IsNeg {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    Or(a=in[15], b=false, out=out);
}
